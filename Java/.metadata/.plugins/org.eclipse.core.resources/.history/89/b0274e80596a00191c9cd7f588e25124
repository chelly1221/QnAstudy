
public class VarEx1 {

	public class VarEx2 {
	
		public static void main(String[] args) {
			int x = 10, y = 20;
			int tmp=0;  //temporary 임시로 x값 담을 변수
			
			System.out.println("x:"+x+"y:"+y);
			
			tmp=x;
			x=y;
			y=tmp;
			
			System.out.println("x:"+x+"y:"+y);
		}
	}

	public static void main(String[] args) {
		int year = 0 ;
		int age = 14 ;
		
		System.out.println(year);
		System.out.println(age);
		
		year = age + 2000;
		age = age + 1;
		
		System.out.println(year);
		System.out.println(age);
	}
}