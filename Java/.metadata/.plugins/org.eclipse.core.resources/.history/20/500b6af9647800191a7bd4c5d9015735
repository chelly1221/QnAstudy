public class FlowEx33 {
	public static void main(String[] args) {
		Loop1 : for(int i=2;i<=9;i++) {    //for문에 이름붙이기 신기방기
		for(int j=2;j<=9;j++) {
			if(j==5)
			break Loop1;
		//	break;
	//		continue Loop1;
//			continue;
			System.out.println(i+"*"+j+"="+ i*j);
		}
		System.out.println();
		}
	}
}