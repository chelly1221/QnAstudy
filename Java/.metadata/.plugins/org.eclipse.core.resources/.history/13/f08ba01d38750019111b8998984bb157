public class OperatorEx22 {
	public static void main(String[] args) {
		float f = 0.1f;
		double d = 0.1;
		double d2 = (double)f;
		
		System.out.printf("10 == 10.0f   \t %b%n", 10==10.0f);
		System.out.printf("'0'==0        \t %b%n", '0'== 0);
		System.out.printf("'A'==65       \t %b%n", '0'== 0);
		System.out.printf("'A' > 'B'     \t %b%n", '0'== 0);
		System.out.printf("'A'+1 != 'B'  \t %b%n", '0'== 0);
		System.out.printf("'A'+1 != 'B'  \t %b%n", '0'== 0);
		System.out.printf("'A'+1 != 'B'  \t %b%n", '0'== 0);
		System.out.printf("'A'+1 != 'B'  \t %b%n", '0'== 0);
		System.out.printf("'A'+1 != 'B'  \t %b%n", '0'== 0);
	}
}